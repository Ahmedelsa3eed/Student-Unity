{
  "name": "student-unity-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepare": "cd .. && husky install Student-Unity-frontend/.husky",
    "format:write": "prettier --config .prettierrc --write \"src/**/*.{js,jsx,ts,tsx,md,html,css}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.2",
    "ng-otp-input": "^1.8.5",
    "ng2-bootstrap": "^0.53.0",
    "ngx-bootstrap": "^8.0.0",
    "ngx-cookie-service": "^13.0.1",
    "ngx-material-timepicker": "^5.5.3",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.10",
    "@angular/cli": "~13.0.1",
    "@angular/compiler-cli": "~13.0.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "husky": "^8.0.2",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.1",
    "typescript": "~4.4.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,md,html,css}": "npm run format:write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"    
    }
  }
}
